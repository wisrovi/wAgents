                <w:t>Copy and Paste This Alias:</w:t>
            </w:r>
        </w:p>
        <w:p>
            <w:r>
                <w:t>alias wisrovi="docker run --rm --hostname wAgent --init -i -t --shm-size=16g --cpus 6.0 --memory 16g --gpus all --log-opt max-size=50m -e TZ=Europe/Madrid -v \"$(pwd)\":/app -v /var/run/docker.sock:/var/run/docker.sock -v ~/.ssh:/root/.ssh:ro wisrovi/agents:gpu-slim zsh"</w:t>
            </w:r>
        </w:p>
        <w:p>
            <w:pPr>
                <w:pStyle w:val="Heading2"/>
            </w:pPr>
            <w:r>
                <w:t>Then simply run: wisrovi</w:t>
            </w:r>
